    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name="com.androidsample.applevantamientoinventario.FormulariosList"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/formulariosActivityName" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.androidsample.applevantamientoinventario.DinamicForm"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_dinamic_form" >
        </activity>
    </application>

</manifest>